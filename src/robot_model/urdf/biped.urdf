<?xml version="1.0"?>
<robot name="serial_manipulator">
    <!-- Base Link-->
    <link name="Base_Link">
        <visual>
            <geometry>
                <cylinder radius="5" length="5"/>
            </geometry>
            <material>
                <color rgba="0 0 1 1">
            </material>
        </visual>
        <collision>
            
        </collision>
        <!-- <inertial>
            <mass value="234.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial> -->
    </link>

    <!-- Serial Link One-->

    <link name="Serial_link_one">
        
    </link>

    <!-- First Joint -->
    <joint name="Base_joint" type="Rotary">
        <parent link="Base_Link"/>
        <child link="Serial_Link_one"/>
        <origin xyz="0.0 0.0 10.0" rpy="0.0 0.0 0.0"/>
        <axis xyz="0.0 0.0 8.0"/>
        <limit lower="0.0" upper="${2*PI}" effort="100.0" velocity="30.0"/>
    </joint>
    
    <!-- Serial Link two-->
    <link name="Serial_Link_two">
        <visual>
            <geometry>
                <cylinder radius="2.5" length="10"/>
            </geometry>
            <material>
                <color rgba="0 1 0 1">
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="2.5" length="10.0"/>
            </geometry>
        </collision>
        <!-- <inertial>
            <mass value="234.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial> -->
    </link>

    <!-- Joint_Two-->

    <joint name="joint_name" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="parent_link"/>
        <child link="child_link"/>
        <axis xyz="0.0 0.0 0.0"/>
        <limit lower="0.0" upper="${PI}.0" effort="100.0" velocity="30.0"/>
    </joint>

    <!-- Serial Link three-->

    <link name="Serial_link_three">

    </link>

    
</robot>

